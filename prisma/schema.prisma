// Prisma schema definition for the M-M_MRP data layer.
// Update this file with models that mirror the structure of your database.

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// Prisma requires at least one model definition to generate a client.
// This placeholder model ensures the Prisma Client is generated even though
// the application uses raw SQL queries for data access.
model PrismaPlaceholder {
  id Int @id
}
